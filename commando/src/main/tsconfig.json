{
  "compilerOptions": {
    "target": "es2017",
    "jsx": "react",
    "lib": [
      "es5",
      "es6",
      "es7",
      "dom"
    ],
    "declaration": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "module": "commonjs",
    "sourceMap": true,
    "strict": true,
    "typeRoots": [
      "../types"
    ]
  },
  "esModuleInterop": true,
  "module": "ES2020"
}
