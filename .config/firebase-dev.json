{
  "functions": {
    "source": "app-backend",
    "ignore": [
      "src",
      ".config",
      "dist-test",
      "deploy.js",
      "node_modules"
    ],
    "predeploy": [
      "echo \"Trying to deploy DEV env... Failing\" && exit 2"
    ]
  },
  "hosting": {
    "public": "app-frontend/dist/dev",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "emulators": {
    "ui": {
      "enabled": true,
      "port": "5555"
    },
    "functions": {
      "port": "5000"
    },
    "hosting": {
      "port": "5009"
    },
    "database": {
      "port": "5002"
    },
    "firestore": {
      "port": "5003"
    }
  }
}
